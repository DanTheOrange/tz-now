{
  "name": "tz-now",
  "version": "1.0.0",
  "description": "What time is it there?",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "rm -rf ./dist && parcel build src/index.html && cp ./now.json ./dist/now.json",
    "predeploy": "npm run build",
    "deploy": "now dist && now alias && now-clear -n tz",
    "precommit": "pretty-quick --staged"
  },
  "keywords": [
    "timezone",
    "now"
  ],
  "author": "Dan Reeves <hey@danreev.es>",
  "license": "ISC",
  "dependencies": {
    "emotion": "^9.2.6",
    "husky": "^0.14.3",
    "luxon": "^1.3.3",
    "memoize-one": "^4.0.0",
    "react": "^16.4.2",
    "react-autocomplete": "^1.8.1",
    "react-dom": "^16.4.2",
    "react-emotion": "^9.2.6",
    "react-select": "^2.0.0"
  },
  "devDependencies": {
    "babel-plugin-emotion": "^9.2.6",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "get-tz-ids": "^1.0.0",
    "now-clear": "0.0.3",
    "parcel-bundler": "^1.9.7",
    "prettier": "^1.14.0",
    "pretty-quick": "^1.6.0"
  }
}
